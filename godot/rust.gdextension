[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.4
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../target/debug/libtbol.so"
linux.release.x86_64 =   "res://../target/release/libtbol.so"
windows.debug.x86_64 =   "res://../target/debug/tbol.dll"
windows.release.x86_64 = "res://../target/release/tbol.dll"
macos.debug =            "res://../target/debug/libtbol.dylib"
macos.release =          "res://../target/release/libtbol.dylib"
macos.debug.arm64 =      "res://../target/debug/libtbol.dylib"
macos.release.arm64 =    "res://../target/release/libtbol.dylib"